{
  "expo": {
    "name": "Wajba",
    "slug": "wajba",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wajba_logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/wajba_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#14776F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wajba.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Wajba needs your location to show nearby restaurants and calculate accurate delivery times.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Wajba needs your location to show nearby restaurants and calculate accurate delivery times."
      },
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wajba_logo.png",
        "backgroundColor": "#14776F"
      },
      "package": "com.wajba.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Wajba to use your location to show nearby restaurants and calculate delivery times."
        }
      ]
    ]
  }
}
